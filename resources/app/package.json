{
  "name": "DofusTouchNE",
  "version": "3.3.2",
  "description": "Emulateur open source de dofus touch pour Linux/OS X/Window",
  "main": "out/electron/main.js",
  "scripts": {
    "start": "electron out/electron/main.js --devmode",
    "dist-mac": "build --mac",
    "dist-linux": "build --linux --x64 --ia32",
    "dist-win": "build --win --x64 --ia32",
    "i18n": "ng-xi18n",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [],
  "author": "Daniel",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/flex-layout": "~2.0.0-beta.4",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/router": "3.4.6",
    "@exponent/electron-cookies": "~2.0.0",
    "@ng-bootstrap/ng-bootstrap": "~1.0.0-alpha.20",
    "angular-sortablejs": "~1.3.2",
    "async": "~2.1.4",
    "bootstrap": "~4.0.0-alpha.6",
    "command-line-args": "~3.0.5",
    "core-js": "~2.4.1",
    "crypto": "~0.0.3",
    "decompress": "~4.0.0",
    "decompress-targz": "~4.0.0",
    "decompress-unzip": "~4.0.1",
    "electron-is-accelerator": "~0.1.1",
    "electron-localshortcut": "~1.0.0",
    "electron-settings": "~2.2.2",
    "electron-simple-updater": "~1.2.0",
    "eventemitter3": "~2.0.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "~2.0.0",
    "keymaster": "~1.6.2",
    "marked": "~0.3.6",
    "neutrino-metrics": "^0.2.10",
    "ng2-tagsinput": "~0.1.4",
    "ng2-translate": "~5.0.0",
    "node-translate": "0.0.4",
    "reflect-metadata": "~0.1.3",
    "request": "~2.79.0",
    "request-progress": "~3.0.0",
    "rxjs": "5.1.0",
    "socket.io-client": "~1.7.3",
    "sweetalert2": "~6.4.3",
    "systemjs": "0.19.27",
    "ts-helpers": "~1.1.1",
    "tslint": "~4.1.1",
    "zone.js": "~0.7.6"
  },
  "devDependencies": {
    "@types/async": "^2.0.37",
    "@types/core-js": "^0.9.35",
    "@types/electron": "^1.4.30",
    "@types/eventemitter3": "^1.2.0",
    "@types/keymaster": "^1.6.28",
    "@types/node": "^6.0.52",
    "@types/request": "0.0.39",
    "@types/socket.io-client": "^1.4.29",
    "canonical-path": "0.0.2",
    "del": "^2.2.2",
    "electron": "1.4.15",
    "electron-builder": "^7.15.2",
    "electron-reload": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-jsbeautifier": "^2.0.4",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.3",
    "lodash": "^4.11.1",
    "replace": "^0.3.0",
    "run-sequence": "^1.2.2",
    "tslint": "*",
    "typescript": "^2.1.4"
  },
  "repository": {},
  "build": {
    "copyright": "Daniel LEFEVBRE",
    "productName": "DofusTouchNE",
    "asar": false,
    "appId": "com.electron.${name}",
    "files": [
      "out/**/*",
      "node_modules/**/*",
      "i18n/**/*",
      "package.json",
      "LICENCE",
      "CHANGELOG.md"
    ],
    "extraFiles": [],
    "mac": {
      "target": [
        "default"
      ],
      "category": "public.app-category.games"
    },
    "linux": {
      "executableName": "DofusTouchNE",
      "target": [
        "tar.gz"
      ],
      "maintainer": "Daniel LEFEVBRE"
    },
    "win": {
      "target": [
        "zip"
      ],
      "iconUrl": "http://dofustouch.no-emu.com/icon.ico"
    }
  }
}
